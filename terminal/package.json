  "author": "AutoClaude Team",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "node-pty": "^1.0.0",
    "winston": "^3.11.0",
    "conf": "^10.2.0",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "figlet": "^1.7.0",
    "update-notifier": "^5.1.0",
    "uuid": "^9.0.1",
