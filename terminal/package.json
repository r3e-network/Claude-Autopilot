{
  "name": "@r3e/autoclaude",
  "version": "3.1.8",
  "description": "Powerful terminal-based AutoClaude AI automation tool with parallel agents and intelligent task management",
  "main": "src/index.ts",
  "bin": {
    "autoclaude": "./bin/autoclaude",
    "cap": "./bin/autoclaude"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js"
  },
  "files": [
    "dist/",
    "bin/",
    "scripts/",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "autoclaude",
    "claude",
    "ai",
    "automation",
    "terminal",
    "cli",
    "parallel-processing",
    "agent-orchestration",
    "task-management",
    "anthropic"
